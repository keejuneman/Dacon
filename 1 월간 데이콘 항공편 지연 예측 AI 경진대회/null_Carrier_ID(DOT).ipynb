    "\n",
    "tail_numbers = train['Tail_Number']\n",
    "tail_alpha = []\n",
    "\n",
    "for tail_number in tail_numbers:\n",
    "    match = re.search(r'\\d([A-Za-z]+)$', tail_number)\n",
    "    if match:\n",
    "        tail_alpha.append(match.group(1))\n",
    "    else:\n",
    "        tail_alpha.append(None)\n",
    "train['tail_alpha'] =  tail_alpha\n"
   ]
  },
