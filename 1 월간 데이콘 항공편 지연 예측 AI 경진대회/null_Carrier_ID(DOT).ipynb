   "source": [
    "pairs = list(zip(train['Airline'], train['Carrier_ID(DOT)']))\n",
    "\n",
    "unique_pairs = {}\n",
    "for pair in pairs:\n",
    "    if not any(pd.isna(p) for p in pair):\n",
    "        unique_pairs[pair] = None\n",
    "\n",
    "unique_pairs_list = list(unique_pairs.keys())\n",
    "\n",
    "\n",
    "air_dict = {}\n",
    "for i in unique_pairs_list:\n",
    "    air_dict[i[0]] = i[1]"
   ]
